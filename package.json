{
  "name": "react200",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "formsy-react": "^1.1.4",
    "json-server": "^0.12.2",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.27.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.3",
    "react-scripts": "^1.1.4"
  },
  "scripts": {
    "start": "run-p start:dev start:api",
    "start:dev": "react-scripts start",
    "start:api": "json-server --port 3001 data/people.json",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
